{% extends 'base.html.twig' %}

{% block title %}Acceuil!{% endblock %}

{% block body %}
<div class="container">
<div id="carouselExampleFade" class="carousel slide carousel-fade" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://www.europeanscientist.com/wp-content/uploads/thumbs/donne%CC%81e-personnelle-sante%CC%81-39lbakv0bvx1d3gbuws83k.png" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://www.aphp.fr/sites/default/files/big_data_en_sante.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://www.innovation-mutuelle.fr/wp-content/uploads/2019/12/donnes-de-sante-conseil-detat.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://images.ladepeche.fr/api/v1/images/view/61093e383e454660e64f0156/hd/image.jpg?v=1" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
</div> 
{% endblock %}