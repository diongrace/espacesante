{% extends 'base.html.twig' %}

{% block title %}Acceuil!{% endblock %}

{% block body %}
<div class="container">
<div id="carouselExampleFade" class="carousel slide carousel-fade" data-bs-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img src="https://www.europeanscientist.com/wp-content/uploads/thumbs/donne%CC%81e-personnelle-sante%CC%81-39lbakv0bvx1d3gbuws83k.png" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://www.aphp.fr/sites/default/files/big_data_en_sante.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://www.innovation-mutuelle.fr/wp-content/uploads/2019/12/donnes-de-sante-conseil-detat.jpg" class="d-block w-100" alt="...">
    </div>
    <div class="carousel-item">
      <img src="https://images.ladepeche.fr/api/v1/images/view/61093e383e454660e64f0156/hd/image.jpg?v=1" class="d-block w-100" alt="...">
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
</br>
<div class="progress">
  <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%;"></div>
</div>
<div class="card-group">
  <div class="card">
    <img src="https://www.mariefrance.fr/wp-content/uploads/sites/5/2015/03/shutterstock_78136399.jpg" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title">CONSULTATIONS</h5>
      <p class="card-text">NOUS SOMMES A VOTRE DISPOSITION.</p>
    </div>
  </div>
  <div class="card">
    <img src="https://comps.gograph.com/medical-emergency_gg64317930.jpg" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title">URGENCES</h5>
      <p class="card-text">EN CAS D'URGENCES DISPONIBLE 24/4H.</p>
    </div>
  </div>
  <div class="card">
    <img src="https://www.tullamorevetclinic.ie/custom/public/images/.gallery4x3/hospitalisation.jpg" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title">HOSPITALISATIONS</h5>
      <p class="card-text">NOUS AVONS DES CHAMBRES DISPONIBLE.</p>
    </div>
  </div>
</div>

</div> 
{% endblock %}